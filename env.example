# Temporal Configuration
TEMPORAL_SERVER_URL=localhost:7233
TEMPORAL_NAMESPACE=default
WORKFLOW_TASK_QUEUE=default
# For Temporal Cloud connections:
# TEMPORAL_API_KEY=your-temporal-cloud-api-key
# TEMPORAL_TLS=true

# Temporal Worker Versioning Configuration
# Enable worker versioning to identify worker deployments and manage versioning behavior
# TEMPORAL_WORKER_USE_VERSIONING=true
# Build ID for this worker deployment (defaults to package.json version if not set)
# TEMPORAL_WORKER_BUILD_ID=1.0
# Deployment name to identify this worker (defaults to 'workflows_worker' if not set)
# TEMPORAL_WORKER_DEPLOYMENT_NAME=workflows_worker
# Default versioning behavior: UNSPECIFIED, PINNED, or AUTO_UPGRADE (defaults to UNSPECIFIED)
# TEMPORAL_WORKER_VERSIONING_BEHAVIOR=UNSPECIFIED

