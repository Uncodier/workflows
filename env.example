# Temporal Configuration
TEMPORAL_SERVER_URL=localhost:7233
TEMPORAL_NAMESPACE=default
WORKFLOW_TASK_QUEUE=default
# For Temporal Cloud connections:
# TEMPORAL_API_KEY=your-temporal-cloud-api-key
# TEMPORAL_TLS=true

# Temporal Worker Versioning Configuration
# REQUIRED: Enable worker versioning (set to "true" to enable)
TEMPORAL_WORKER_USE_VERSIONING=true
# REQUIRED: Deployment name to identify this worker (appears in Temporal UI)
TEMPORAL_WORKER_DEPLOYMENT_NAME=workflows_worker
# OPTIONAL: Build ID (defaults to package.json version if not set)
# Examples: "0.2.6", "v1.0.0", "commit-abc123"
# TEMPORAL_WORKER_BUILD_ID=0.2.6
# OPTIONAL: Default versioning behavior: PINNED, AUTO_UPGRADE, or UNSPECIFIED
# If not set or UNSPECIFIED, no default behavior is set (each workflow specifies its own)
# TEMPORAL_WORKER_VERSIONING_BEHAVIOR=UNSPECIFIED

# Supabase Configuration
SUPABASE_URL=
SUPABASE_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# API Configuration
API_BASE_URL=
API_KEY=

# Application Configuration
LOG_LEVEL=info 